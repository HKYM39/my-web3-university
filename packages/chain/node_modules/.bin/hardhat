#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/hkym/Code/test/web3-university-monorepo/node_modules/.pnpm/hardhat@3.1.0/node_modules/hardhat/dist/src/node_modules:/home/hkym/Code/test/web3-university-monorepo/node_modules/.pnpm/hardhat@3.1.0/node_modules/hardhat/dist/node_modules:/home/hkym/Code/test/web3-university-monorepo/node_modules/.pnpm/hardhat@3.1.0/node_modules/hardhat/node_modules:/home/hkym/Code/test/web3-university-monorepo/node_modules/.pnpm/hardhat@3.1.0/node_modules:/home/hkym/Code/test/web3-university-monorepo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/hkym/Code/test/web3-university-monorepo/node_modules/.pnpm/hardhat@3.1.0/node_modules/hardhat/dist/src/node_modules:/home/hkym/Code/test/web3-university-monorepo/node_modules/.pnpm/hardhat@3.1.0/node_modules/hardhat/dist/node_modules:/home/hkym/Code/test/web3-university-monorepo/node_modules/.pnpm/hardhat@3.1.0/node_modules/hardhat/node_modules:/home/hkym/Code/test/web3-university-monorepo/node_modules/.pnpm/hardhat@3.1.0/node_modules:/home/hkym/Code/test/web3-university-monorepo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../hardhat/dist/src/cli.js" "$@"
else
  exec node  "$basedir/../hardhat/dist/src/cli.js" "$@"
fi
